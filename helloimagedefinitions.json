{
  "taskDefinition": {
    "family": "hello-demo",
    "containerDefinitions": [
      {
        "name": "hello-demo",
        "image": "709325198486.dkr.ecr.us-east-1.amazonaws.com/hello-world:latest",
        "cpu": 1024,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "memory": 512,
        "essential": true
      }
    ]
  }
}